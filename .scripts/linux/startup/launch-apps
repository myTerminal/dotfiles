#!/usr/bin/env bash

command=$1

case "${1}" in
    e | express )
        exec emacs & disown
        exec chromium & disown
        exec logseq & disown
        exec keepassxc & disown
        ;;

    g | gaming )
        exec steam & disown
        ;;

    *)
        exec signal-desktop & disown
        exec Telegram & disown
        exec emacs & disown
        exec firefox & disown
        exec logseq & disown
        exec better-bird & disown
        exec keepassxc & disown
        ;;
esac
