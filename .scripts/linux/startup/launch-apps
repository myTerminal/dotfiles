#!/usr/bin/env bash

command=$1

case "${1}" in
    e | express )
        exec logseq & disown
        exec emacs & disown
        exec chromium & disown
        exec keepassxc & disown
        ;;

    g | gaming )
        exec steam & disown
        ;;

    *)
        exec logseq & disown
        exec emacs & disown
        exec schildi & disown
        exec signal-desktop & disown
        exec telegram-desktop & disown
        exec firefox & disown
        exec thunar & disown
        exec thunderbird & disown
        exec keepassxc & disown
        ;;
esac
